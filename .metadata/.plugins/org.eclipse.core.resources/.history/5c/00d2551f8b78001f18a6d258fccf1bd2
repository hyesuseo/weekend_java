package loop;

public class Ex06 {

	public static void main(String[] args) {
		// 이중 for문과 * 하나를 사용하여 별모양 만들기...??
		// system.out.print("*");
		// ***** 다섯줄 만들기 다섯번 출력하고 줄바꿈 
		
		
		
		System.out.print("*");
		
		
		for(int i=0; i <5 ; i++) { //초기화를 어떻게 해야하는지 모르겠다..??ㅋㅋ
			
			for(int j = 0; j<5; j++) { System.out.print("*");	
		}
		
System.out.println();
	}

}
