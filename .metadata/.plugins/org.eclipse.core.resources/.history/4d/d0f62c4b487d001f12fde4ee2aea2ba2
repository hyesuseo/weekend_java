package array;

public class Ex09 {

	public static void main(String[] args) {
		int[] score = new int[] {60, 77, 84, 100, 94};
		
		//향상된 for 문: 사용이 많이 되므로 잘 알아두어야한다.
		for(int i : score) {
			//배열의 요소를 저장할 변수선언 : (콜론) 배열 
		System.out.print(i + " ");
		}
		System.out.println();
		
		
		//배열 복사
		int[] newScore = new int[7];
		for (int i = 0; i<score.length; i++) {
			newScore[i] = score[i];
		}
		//향상된 for문으로 출력해보기
		
		for (int i : newScore) {
			System.out.print(i + " ");
		}

	}

}
