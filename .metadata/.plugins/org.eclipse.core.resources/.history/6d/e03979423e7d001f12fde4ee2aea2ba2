package array;

import java.util.Scanner;

public class Ex07 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int[] num = new int[5];
		int temp =0;
		//5개의 숫자를 입력받아, 작은 수부터 나열하기
		// 배열 for문 if문 scanner 사용
		
		for(int i= 0; i <5; i++) {
			System.out.print(i+1+"번째 숫자: ");
			num[i]=scan.nextInt();
			
		}
	}

}
