package random;

public class Ex02 {

	public static void main(String[] args) {
		int[] lotto = new int[6];
		//정수 1~45까지 6개 추첨
		//Math.random을 사용하여
		//로또 번호를 뽑아 lotto배열의 각 요소에 저장하기
		
		for (int i=0; i<6; i++ ) {
			
			lotto[i]=(int)(Math.random()*45)+1;
			//중복된 수가 나오면 안되니까...
			for(int j = 0 ; i==j; j++) {
				lotto[i]=(int)(Math.random()*45)+1;
			}
		
		}
		for (int i : lotto) {
			System.out.print(i+" ");
		}
	
			
	}

}
